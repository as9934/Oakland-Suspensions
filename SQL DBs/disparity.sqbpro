tional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="SQL 1">SELECT black_susp.Name, white_susp.school_year,  black_susp.UnduplicatedCountofStudentsSuspended as black_kids_suspended, black_susp.CumulativeEnrollment as black_enrollment, black_susp.black_susp_rate, white_susp.UnduplicatedCountofStudentsSuspended as white_kids_suspended, white_susp.CumulativeEnrollment as white_enrollment, white_susp.white_susp_rate, CAST(black_susp.black_susp_rate AS float)  / white_susp.white_susp_rate as disparity, CAST(black_susp.black_susp_rate AS float)  - white_susp.white_susp_rate as difference_disparity
